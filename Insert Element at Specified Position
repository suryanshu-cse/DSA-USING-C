#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
int n;
int list[100];
void insertatspecificposition(int position,int value){
 for(int i=n-1;i>=position;i--){
     list[i+1]=list[i];
}
    list[position]=value;
    n++;
}
int main() {
scanf("%d",&n);
    for(int i=0;i<n;i++){
        scanf("%d",&list[i]);
    }
    int position;
    int value;
    scanf("%d",&value);
    scanf("%d",&position);
    
    insertatspecificposition(position,value);
    
for(int i=0;i<n;i++){
    printf("%d ",list[i]);
}
    return 0;
}
