#include <stdio.h>

int n;
int list[100];

void insertAtPosition(int value, int position) {
    for (int i = n - 1; i >= position; i--) {
        list[i + 1] = list[i];
    }
    list[position] = value;
    n++;
}

int main() {

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &list[i]);
    }

    int operations;  
    scanf("%d", &operations);

    for (int i = 0; i < operations; i++) {
        int value, position;  
        scanf("%d %d", &value, &position);

        insertAtPosition(value, position);
    }

    for (int i = 0; i < n; i++) {
        printf("%d ", list[i]);
    }

    return 0;
}
